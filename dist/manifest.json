{
    "name": "ulthar",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "ulthar is your private enclave.",
    "homepage_url": "https://duo.com/labs",
    "icons": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    },
    "options_ui" :{
      "page": "options.html",
      "open_in_tab": false,
      "scripts": [
        "js/options.js"
      ]
    },
    "browser_action": {
      "default_title": "TSRWPCX",
      "default_popup": "popup.html"
    },
    "default_locale": "en",
    "background": {
      "scripts": [
        "js/background.js"
      ],
      "persistent": true
    },
    "permissions": [
      "https://*/*",
      "tabs",
      "storage"
    ],
    "content_security_policy": "default-src 'self' 'unsafe-eval' https://5a310dbedc2f.ngrok.io;"
  }